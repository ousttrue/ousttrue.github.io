{% extends 'base.tmpl' %}
{% import 'post_title.tmpl' as post_title with context %}

{% block content %}
{% if prevlink %}
<a href="{{ prevlink }}" rel="prev">👉未来</a>
{% endif %}

{% for post in posts %}
{{ post_title.headline(post) }}
{% endfor %}

{% if nextlink %}
<a href="{{ nextlink }}" rel="next">👈過去</a>
{% endif %}
{% endblock %}
