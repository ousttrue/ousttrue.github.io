    "version": "0.2.0",
    "configurations": [
        {
            "arg": [],
            "name": "lua",
            "program": "${workspaceFolder}/site.lua",
            "request": "launch",
            "stopOnEntry": false,
            "luaVersion": "jit",
            "type": "lua"
        },
        {
            "name": "sphinx",
            "type": "python",
            "request": "launch",
            "module": "sphinx",
            "args": [
                "content",
                "build",
            ],
            "justMyCode": false,
            "env": {
                "LANG": "C",
            }
        },
        {
            "name": "nikola",
            "type": "python",
            "request": "launch",
            "module": "nikola",
            "args": [
                "build"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/replace.py",
            "console": "integratedTerminal"
        }
    ]
}